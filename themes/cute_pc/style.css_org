@-webkit-keyframes scale {
    25% {
    	-webkit-transform: rotate(180deg);
    }
    60% {
    	left: 35%;
        -webkit-transform: rotateX(360deg);
    }
    75% {
        -webkit-transform: rotateX(180deg);
    }
    100% {
    	left: 22%;
        -webkit-transform: rotateX(360deg);
    }
}
@keyframes scale {
    25% {
    	transform: rotate(180deg);
    }
    60% {
    	left: 35%;
        transform: rotateX(360deg);
    }
    75% {
        transform: rotateX(180deg);
    }
    100% {
    	left: 22%;
        transform: rotateX(360deg);
    }
}

@-webkit-keyframes scaleBack { /* No Use */
    100% {
    	left: -150px;
    	//opacity: 0;
    }
}
@keyframes scaleBack { /* No Use */
    100% {
    	left: -150px;
    	//opacity: 0;  
    }
}

.onOff {
    -webkit-animation: scale 0.6s linear 0.1s 1 normal forwards;
    animation: scale 0.6s linear 0.1s 1 normal forwards;
}
.onOffBack {
	-webkit-animation: scaleBack 0.35s linear 0.1s 1 normal forwards;
    animation: scaleBack 0.35s linear 0.1s 1 normal forwards;
}



