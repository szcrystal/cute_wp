    //border: 1px solid orange;
}
.main-navigation ul li:last-of-type {
	margin-right: 0;
    //min-height: 3em;
}
.main-navigation a {
	//display: block;
    //color: #4FBFC1;
    font-weight: 600;
    font-size: 1.0em;
    color: #3f3f3f;
    padding: 0 2px;
	text-decoration: none;
    text-transform: uppercase;
}
.main-navigation a + span {
	position: absolute;
    top: 1.4em;
    left: 0;
    background: #2a2a2a;
    height: 1px;
    width: 100%;
    -webkit-transform: scale(0, 1);
    transform: scale(0, 1);
    -webkit-transition: all linear .3s;
    transition: all linear .3s;
}
.main-navigation a:hover + span {
	-webkit-transform: scale(1);
    transform: scale(1); 
}
.main-navigation a.current + span {
	-webkit-transform: scale(1);
    transform: scale(1); 
}
